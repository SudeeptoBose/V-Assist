<AnchorPane id="AnchorPane" prefHeight="635.0" prefWidth="969.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.test.pkg1.CreateAccountController">
    <stylesheets>
        <URL value="@createaccount.css" />
    </stylesheets>
   <children>
      <Pane layoutX="195.0" layoutY="160.0" prefHeight="336.0" prefWidth="452.0">
         <children>
            <JFXTextField fx:id="txtFirstName" layoutX="137.0" layoutY="14.0" onAction="#getFirstName" prefHeight="32.0" prefWidth="302.0" promptText="First Name">
