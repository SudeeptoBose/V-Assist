                  <Font size="18.0" />
               </font></JFXTextField>
            <JFXPasswordField fx:id="txtPassword" layoutX="137.0" layoutY="200.0" onAction="#getPassword" prefHeight="32.0" prefWidth="302.0" promptText="Password">
