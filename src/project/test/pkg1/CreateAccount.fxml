            <JFXTextField fx:id="txtConfirm" alignment="CENTER" layoutX="302.0" layoutY="265.0" promptText="Confirm" />
            <Label fx:id="loginLabel" alignment="CENTER" layoutX="137.0" layoutY="336.0" onMouseClicked="#handleLabelAction" text="Login instead" textFill="#3c6bda" />
