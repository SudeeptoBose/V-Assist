<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="635.0" prefWidth="969.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.test.pkg1.ForgotPasswordController">
   <children>
      <JFXTextField fx:id="uname" layoutX="322.0" layoutY="212.0" onAction="#getuname" prefHeight="54.0" prefWidth="344.0" promptText="Enter Username">
         <font>
            <Font name="Microsoft JhengHei UI Light" size="26.0" />
         </font>
      </JFXTextField>
      <JFXButton fx:id="update" layoutX="695.0" layoutY="506.0" onAction="#setupdate" prefHeight="70.0" prefWidth="168.0" style="-fx-background-color: #1c6eb5; -fx-background-radius: 50px;" text="Update" textFill="WHITE">
         <font>
            <Font name="Microsoft JhengHei" size="24.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></JFXButton>
      <JFXButton fx:id="back" layoutX="138.0" layoutY="506.0" onAction="#goback" prefHeight="70.0" prefWidth="168.0" style="-fx-border-color: #1c6eb5; -fx-border-radius: 50px;" text="Back">
         <font>
            <Font name="Microsoft JhengHei Light" size="24.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></JFXButton>
      <Label layoutX="264.0" layoutY="14.0" text="Forgot Password?">
         <font>
            <Font name="Microsoft JhengHei Light" size="55.0" />
         </font>
      </Label>
      <Label layoutX="236.0" layoutY="92.0" text="How could you forget your password? ">
         <font>
            <Font name="Microsoft JhengHei Light" size="28.0" />
         </font>
      </Label>
      <JFXPasswordField fx:id="npass" layoutX="322.0" layoutY="306.0" onAction="#getnpass" prefHeight="54.0" prefWidth="344.0" promptText="New Password">
         <font>
            <Font name="Microsoft JhengHei Light" size="26.0" />
         </font>
      </JFXPasswordField>
      <JFXPasswordField fx:id="cpass" layoutX="322.0" layoutY="406.0" onAction="#getcpass" prefHeight="54.0" prefWidth="344.0" promptText="Confirm Password">
         <font>
            <Font name="Microsoft JhengHei Light" size="26.0" />
         </font>
      </JFXPasswordField>
   </children>
</AnchorPane>
